<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.next {
			float: right;
		}
		.defaultheight {
			min-height: 400px;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:HTMLPanel addStyleNames="{style.defaultheight}">
			<div style="width: 100%; display: table;">
				<h1 style="float: left;">Network configuration</h1>
			</div>
		</g:HTMLPanel>
		<div>
			<g:InlineHyperlink ui:field="previousLink">previous step</g:InlineHyperlink>
			<g:InlineHyperlink addStyleNames="{style.next}" ui:field="nextLink">next step</g:InlineHyperlink>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 