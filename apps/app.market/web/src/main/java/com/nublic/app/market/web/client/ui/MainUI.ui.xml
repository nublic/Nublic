<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:n="urn:import:com.nublic.app.market.web.client.ui">
	<ui:style>
		.scroll {
			overflow-y: auto;
		}
		.toppadding {
			padding-top: 10px;
		}
	</ui:style>
	<g:DeckPanel ui:field="deckPanel" height="100%" width="100%">
		<g:SimplePanel addStyleNames="{style.scroll} {style.toppadding}">
			<n:AppPanel ui:field="appPanel" addStyleNames="container"/>
		</g:SimplePanel>
		<n:OpenPanel ui:field="openPanel"/>
	</g:DeckPanel>
</ui:UiBinder> 