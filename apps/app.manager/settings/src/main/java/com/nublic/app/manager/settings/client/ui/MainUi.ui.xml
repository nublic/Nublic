<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:n="urn:import:com.nublic.app.manager.settings.client.ui">
	<ui:style>
		.leftPanelPadding {
			padding: 10px 10px 30px 10px;
		}
	</ui:style>

	<g:DockLayoutPanel>
		<g:west size="220">
			<g:HTMLPanel addStyleNames="{style.leftPanelPadding}">
				<n:NavPanel ui:field="navPanel"/>
			</g:HTMLPanel>
		</g:west>
		<g:center>
			<g:SimplePanel ui:field="centralPanel"/>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>
