<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:com.gwtmobile.ui.client.widgets">
	<ui:style>
	</ui:style>
	<g:HTMLPanel>
		<m:HeaderPanel ui:field="header" leftButton="Back" rightButton="Next">
			<g:Label>Slide Panel</g:Label>			
		</m:HeaderPanel>
		<m:SlidePanel ui:field="slider">
			<m:Slide styleName="Slide Slide-Content">
				<g:HTML>Slide Me!</g:HTML>
				<g:HTML>Static Slide 0</g:HTML>
			</m:Slide>	
			<m:Slide styleName="Slide Slide-Content">
				<g:HTML>Slide Me!</g:HTML>
				<g:HTML>Static Slide 1</g:HTML>
			</m:Slide>	
		</m:SlidePanel>
		
	</g:HTMLPanel>	
</ui:UiBinder> 