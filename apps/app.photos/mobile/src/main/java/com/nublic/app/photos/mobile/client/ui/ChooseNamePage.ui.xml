<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:com.gwtmobile.ui.client.widgets">
	<ui:style>
		.modify > div > div {
			-webkit-box-flex: 1;
			margin: 0 .5em 0 .5em;
		}
	</ui:style>
	
	<ui:with type="com.nublic.app.photos.mobile.client.i18n.I18NMessages" field="msg"/>
	
	<g:HTMLPanel>
		<m:HeaderPanel leftButton="Back">
			<g:Label ui:field="title">Choose a name</g:Label>
		</m:HeaderPanel>
		<m:ScrollPanel>
			<m:ListPanel selectable="false" secondaryStyle="{style.modify}">
				<m:ListItem>
					<m:TextBox ui:field="nameBox"/>
				</m:ListItem>
				<m:ListItem>
					<m:Button ui:field="okButton">OK</m:Button>
				</m:ListItem>
			</m:ListPanel>
		</m:ScrollPanel>
	</g:HTMLPanel>
</ui:UiBinder> 