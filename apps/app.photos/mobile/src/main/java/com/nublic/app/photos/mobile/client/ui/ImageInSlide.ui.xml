<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<ui:with type="com.nublic.app.photos.mobile.client.Resources" field="res"/>

	<g:AbsolutePanel width="100%" height="100%">
	    <g:at left="0" right="0" top="0" bottom="0">
			<g:VerticalPanel width="100%" height="100%" verticalAlignment="middle" horizontalAlignment="center">
		    	<g:Image ui:field="loading" resource="{res.loading}"/>
			</g:VerticalPanel>
		</g:at>
		<g:at left="0" right="0" top="0" bottom="0">
			<g:VerticalPanel ui:field="imagePanel" width="100%" height="100%" verticalAlignment="middle" horizontalAlignment="center"/>
		</g:at>
	</g:AbsolutePanel>
</ui:UiBinder> 