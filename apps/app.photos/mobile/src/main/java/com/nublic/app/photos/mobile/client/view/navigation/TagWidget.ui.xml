<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type='com.nublic.app.photos.web.client.view.navigation.TagWidget.TagStyle'>
		.innerImage {
			vertical-align: middle;
			margin-right: 5px;
		}
		.nobackground {
			background: transparent;
			border-style: none;
			box-shadow: none;
			padding: 0;
			opacity: 0.1;
		}
		.nobackground:hover {
			opacity: 0.6;
		}
	</ui:style>
	<g:HTMLPanel width="100%">
		<ul class="nav nav-list">
			<li>
				<g:InlineHyperlink ui:field="anchor"/>
				<!-- <g:Image ui:field="icon" width="16px" height="16px"/> -->
			</li>
		</ul>
	</g:HTMLPanel>
</ui:UiBinder> 