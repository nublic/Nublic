<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type='com.nublic.app.music.client.ui.song.SongList.SongStyle'>
		.alignright {
			text-align: right;
		}
		.alignmiddle {
			vertical-align: middle;
		}
		.leftmargin {
			margin-left: 15px;
		}
		.rightmargin {
			margin-right: 15px;
		}
		.bottommargin {
			margin-bottom: 15px;
		}
	</ui:style>
	<g:FlowPanel addStyleNames="{style.bottommargin} {style.alignmiddle}">
		<g:Grid ui:field="grid">
		</g:Grid>
	</g:FlowPanel>
</ui:UiBinder> 