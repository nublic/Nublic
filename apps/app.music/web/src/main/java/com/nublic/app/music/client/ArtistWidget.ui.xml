<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	   .inlinecell {
           display: inline-block;
       }
	</ui:style>
	<g:HorizontalPanel>
		<g:HorizontalPanel spacing="5">
			<g:Cell horizontalAlignment="ALIGN_CENTER">
				<g:Image width="96px" height="96px" ui:field="imagen"/>
			</g:Cell>
			<g:VerticalPanel width="100%" height="100%">
				<g:Label text="Id" ui:field="ID"/>
				<g:Label text="Nombre" ui:field="nombre"/>
				<g:FlowPanel width="100%" height="100%" styleName="{style.inlinecell}" ui:field="albums"/>
			</g:VerticalPanel>
	   </g:HorizontalPanel>
	</g:HorizontalPanel>
</ui:UiBinder> 