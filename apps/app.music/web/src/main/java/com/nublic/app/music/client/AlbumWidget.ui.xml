<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	   .inlinecell {
           display: inline-block;
       }
	</ui:style>
	<g:VerticalPanel styleName="{style.inlinecell}" width="96">
	   <g:VerticalPanel styleName="{style.inlinecell}" width="96" spacing="5">
	   	<g:Cell horizontalAlignment="ALIGN_CENTER">
	   		<g:Image width="96px" height="96px" ui:field="albumIm"/>
	   	</g:Cell>
	       <g:HorizontalPanel width="100%" height="100%">
	           <g:Label text="Nombre" ui:field="albumTitle"/>
	       </g:HorizontalPanel>
       </g:VerticalPanel>
    </g:VerticalPanel>
</ui:UiBinder> 