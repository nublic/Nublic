<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to='exampleapp'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.google.gwt.http.HTTP'/>
  <!-- To prevent "required inherited module" error               -->
  <inherits name="com.google.gwt.junit.JUnit"/>
  <inherits name="com.google.common.collect.Collect"/>
  <inherits name="com.google.gwt.resources.Resources" />

  <!-- Internationalization										  -->
  <inherits name="com.google.gwt.i18n.I18N"/>
  <extend-property name="locale" values="en,es"/>
  <!-- default locale... -->
  <set-property-fallback name="locale" value="en"/>
  <set-property name="locale" value="en,es"/>
  <!-- get locale in runtime -->
  <set-configuration-property name="locale.useragent" value="Y"/>

  <!-- Specify the paths for translatable code                    -->
  <source path='i18n'/>
  <source path='gwt'/>
  <source path='graph'/>
  <source path='lattice'/>
  <source path='messages'/>
  <source path='error'/>
  <source path='comparators'/>
  <source path='tuples'/>
  <source path='widgets'/>
  <source path='images'/>
  <source path='range'/>
  <source path='cache'/>

</module>
