<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='browserapp'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.google.gwt.http.HTTP'/>
  <!-- To prevent "required inherited module" error               -->
  <inherits name="com.google.gwt.junit.JUnit"/>
  <inherits name="com.bramosystems.oss.player.core.Core"/>
  <inherits name='com.nublic.util.GwtUtil'/>
  <!-- <inherits name="com.bramosystems.oss.player.youtube.YouTube"/>  --> <!-- To use if YouTubePlayer is used -->
  <inherits name='edu.ycp.cs.dh.acegwt.AceGWT'/>
  <inherits name="com.google.common.collect.Collect"/>
  <inherits name='com.allen_sauer.gwt.dnd.gwt-dnd'/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <!-- <inherits name='com.google.gwt.user.theme.clean.Clean'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
  <inherits name='com.nublic.theme.NublicTheme'/>

  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='com.nublic.app.browser.web.client.BrowserApp'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

</module>
