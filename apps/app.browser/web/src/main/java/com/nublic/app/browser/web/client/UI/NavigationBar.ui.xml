<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style  type='com.nublic.app.browser.web.client.UI.NavigationBar.NavStyle'>
		.padding {
			padding: 5px;
		}
		.labelspadding {
			padding-left: 5px;
			padding-right: 5px;
		}
		.verticalalignmiddle {
			vertical-align: middle;
		}
	</ui:style>
	<g:HorizontalPanel ui:field="rootPanel" verticalAlignment="middle" addStyleNames="{style.padding}">
	</g:HorizontalPanel>
</ui:UiBinder> 