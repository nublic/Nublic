<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.scamall.resource">
  <class name="Key" table="key">
  	<id name="id" column="id">
  		<generator class="native"></generator></id>
  	<property name="keyId" column="key_id"></property>
  	<property name="typeId" column="type_id"></property>
  	<property name="appId" column="app_id"></property>
  	<many-to-one name="app" column="app_id" class="App"></many-to-one>
  	<set name="values" table="value">
  		<key column="key_id"></key>
  		<element type="Value"></element>
  	</set></class>
</hibernate-mapping>