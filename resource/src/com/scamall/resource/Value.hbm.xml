<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.scamall.resource">
  <class name="Value" table="value">
  	<id column="id">
  		<generator class="native"></generator>
  	</id>
  	<property name="id"></property>
  	<property name="key_id"></property>
  	<property name="subkey"></property>
  	<property name="value"></property>
  	<many-to-one name="key" class="Key" column="key_id"></many-to-one></class>
</hibernate-mapping>